{
  "name": "chareice/laravel-tencentcloud",
  "description": "TencentCloud SDK for Laravel framework",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Chareice\\TencentCloud\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Chareice",
      "email": "chareice@live.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "tencentcloud/tencentcloud-sdk-php": "^3.0",
    "laravel/framework": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "^6.21"
  },
  "extra": {
    "laravel": {
      "providers" : [
        "Chareice\\TencentCloud\\ServiceProvider"
      ]
    }
  }
}
